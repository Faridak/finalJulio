# Application Configuration
APP_NAME=VentDepot
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=https://yourdomain.com

# Database Configuration
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=ventdepot
DB_USERNAME=ventdepot_user
DB_PASSWORD=your_strong_password

# Redis Configuration
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_PASSWORD=null
REDIS_DB=0

# Stripe Configuration
STRIPE_KEY=pk_test_your_stripe_public_key
STRIPE_SECRET=sk_test_your_stripe_secret_key

# PayPal Configuration
PAYPAL_CLIENT_ID=your_paypal_client_id
PAYPAL_SECRET=your_paypal_secret
PAYPAL_MODE=sandbox

# Email Configuration
MAIL_DRIVER=smtp
MAIL_HOST=smtp.mailgun.org
MAIL_PORT=587
MAIL_USERNAME=your_mailgun_username
MAIL_PASSWORD=your_mailgun_password
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS=noreply@yourdomain.com
MAIL_FROM_NAME=VentDepot

# Security Configuration
SESSION_LIFETIME=120
SECURITY_SALT=your_random_salt_here
RATE_LIMIT_MAX_REQUESTS=60
RATE_LIMIT_WINDOW=60

# Logging Configuration
LOG_CHANNEL=stack
LOG_LEVEL=debug

# API Configuration
API_RATE_LIMIT=1000
API_RATE_LIMIT_WINDOW=3600

# File Storage
STORAGE_DISK=local
STORAGE_ROOT=storage/app/public

# Caching
CACHE_DRIVER=redis
CACHE_PREFIX=ventdepot

# Session Configuration
SESSION_DRIVER=redis
SESSION_LIFETIME=120

# Queue Configuration
QUEUE_CONNECTION=redis

# Broadcasting
BROADCAST_DRIVER=redis

# Other Services
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_APP_CLUSTER=mt1

MIX_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
MIX_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"